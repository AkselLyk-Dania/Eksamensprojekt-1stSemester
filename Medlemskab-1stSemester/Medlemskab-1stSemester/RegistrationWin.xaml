<Window x:Class="Medlemskab_1stSemester.RegistrationWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medlemskab_1stSemester"
        mc:Ignorable="d"
        Title="RegistrationWin" Height="300" Width="500">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,26,0,0" TextWrapping="Wrap" Text="Vælg et kurs for at tilmelde" VerticalAlignment="Top" Width="200" FontSize="16"/>
        <ListBox x:Name="ActivityList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="80,57,100,85"/>
        <Button x:Name="ToRegistration" Content="Næste" HorizontalAlignment="Left" Margin="287,214,0,0" VerticalAlignment="Top" Width="88" Click="ToRegistration_Click"/>
        <Button x:Name="CancelRegistration" Content="Tilbage" HorizontalAlignment="Left" Margin="113,214,0,0" VerticalAlignment="Top" Width="88" Click="CancelRegistration_Click"/>

    </Grid>
</Window>
