<Window x:Class="Medlemskab_1stSemester.EditUserWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medlemskab_1stSemester"
        mc:Ignorable="d"
        Title="Rediger Bruger" Height="266" Width="450">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,30,0,0" TextWrapping="Wrap" Text="Rediger Medlem" VerticalAlignment="Top" Height="24" Width="128" FontWeight="Bold" FontSize="16"/>
        <TextBlock Grid.Column="0" Text="Nyt Navn:" FontSize="15" VerticalAlignment="Top" Margin="58,69,304,0" Height="24"/>
        <TextBox x:Name="EditUserName" HorizontalAlignment="Center" Margin="0,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="158" Height="30"/>
        <Button x:Name="EditUserConfirm" Content="Godkend" HorizontalAlignment="Center" Margin="0,156,0,0" VerticalAlignment="Top" Width="74" Click="EditUserConfirm_Click"/>
        <Button x:Name="CancelUserEdit" Content="Annuller" HorizontalAlignment="Center" Margin="0,188,0,0" VerticalAlignment="Top" Width="74" Click="CancelUserEdit_Click"/>
        <CheckBox x:Name="EditStudentCheck" Content="Oprettelsen er student" HorizontalAlignment="Left" Margin="131,118,0,0" VerticalAlignment="Top" Height="18" Width="144">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="1.3" ScaleY="1.3"/>
            </CheckBox.LayoutTransform>
        </CheckBox>

    </Grid>
</Window>
