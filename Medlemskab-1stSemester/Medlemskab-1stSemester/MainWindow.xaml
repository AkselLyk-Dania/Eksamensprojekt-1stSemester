<Window x:Class="Medlemskab_1stSemester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Medlemskab_1stSemester"
        mc:Ignorable="d"
        Title="Dansk Data" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Image Source="/Resources/Logo.png" Width="220" Height="157" Margin="0,-20,0,0"/>
        </StackPanel>

        <ListBox x:Name="AddLog" d:ItemsSource="{d:SampleData ItemCount=5}" IsSynchronizedWithCurrentItem="True" Height="209" Width="356"/>
        <ListBox x:Name="MemberList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="40,47,604,178" IsSynchronizedWithCurrentItem="True"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="604,47,40,178"/>
        <Button x:Name="AddUser" Content="Opret Medlem" HorizontalAlignment="Left" Margin="70,281,0,0" VerticalAlignment="Top" Height="19" Width="96" Click="Opret_Bruger_Click"/>
        <Button x:Name="EditUser" Content="Rediger Medlem" HorizontalAlignment="Left" Margin="70,300,0,0" VerticalAlignment="Top" Height="19" Width="96" Click="EditUser_Click"/>
        <Button x:Name="DeleteUser" Content="Slet Medlem" HorizontalAlignment="Left" Margin="70,319,0,0" VerticalAlignment="Top" Height="19" Width="96" Click="DeleteUser_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="634,281,0,0" VerticalAlignment="Top" Height="19" Width="96"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="634,300,0,0" VerticalAlignment="Top" Height="19" Width="96"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="634,319,0,0" VerticalAlignment="Top" Height="19" Width="96"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,336,0,0" VerticalAlignment="Top" Height="23" Width="100"/>
        <TextBlock HorizontalAlignment="Left" Margin="68,21,0,0" TextWrapping="Wrap" Text="Medlemmer:" VerticalAlignment="Top" Width="100" FontSize="16"/>
        <TextBlock HorizontalAlignment="Left" Margin="640,21,0,0" TextWrapping="Wrap" Text="Aktiviteter:" VerticalAlignment="Top" Width="90" FontSize="16"/>
        <Button x:Name="ChangeAdmin" Content="Skift Administrator" HorizontalAlignment="Left" Margin="24,367,0,0" VerticalAlignment="Top" Width="112" Click="ChangeAdmin_Click"/>
    </Grid>
</Window>
